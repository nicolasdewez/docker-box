{
    "name": "nicolasdewez/doker-box",
    "license": "MIT",
    "type": "project",
    "description": "Symfony application to manage local docker container",
    "autoload": {
        "psr-0": { "App\\": "src/", "SymfonyStandard": "app/" }
    },
    "require": {
        "symfony/console": "~2.0",
        "symfony/dom-crawler": "~2.0",
        "symfony/dependency-injection": "~2.0",
        "symfony/config": "~2.0",
        "symfony/finder": "~2.0",
        "symfony/process": "~2.0",
        "symfony/serializer": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
          "dev-master": "2.0.x-dev"
        }
    }
}
